.title_bg {
  background-color: #d1121a;
  height: 40px;
  position: fixed;
  z-index: 9999;
  width: 100%;
}

.title_img {
  float: left;
  height: 40px;
  margin-left: 10px; 
}
.title_btn{border:1px solid #ddd;
color:#fff;
font-size:28rpx;
padding:6rpx 20rpx;
float:right;
margin-right:10px;
margin-top:10rpx;
}

.title_name {
  float: right;
  font-size: 14px;
  color: #fff;
  line-height: 40px;
  margin-right: 10px;
}

.title_tx {
  float: right;
  width: 30px;
  height: 30px;
  margin-top: 5px;
  margin-right: 8px;
}

.list_fl {
  display: inline-block;
  width: 100%;
  margin: 5px 0;
}

.fl {
  float: left;
  width: 20%;
  text-align: center;
}

.fl_name {
  font-size: 12px;
  color: #333;
}

.yhq_fa {
  width: 95%;
  height: auto;
  margin: 0 auto;
}

.yhq_body {
  position: relative;
  background-color: #fff;
  width: 100%;
  height: 90px;
  margin-bottom: 10px;
  overflow: hidden;
}

.yhq_spot {
  position: absolute;
  color: #f0eff4;
  z-index: 998;
  line-height: 8px;
  font-size: 8px;
  left: -3px;
}

.yhq_spot2 {
  position: absolute;
  color: #f0eff4;
  z-index: 998;
  line-height: 8px;
  font-size: 8px;
  right: -3px;
}
.yhq_spot3{position: absolute; color: #f0eff4; z-index: 998; line-height: 8px; font-size: 18px; right: 59.5px;top:-6px;}

.yhq_spot4{position: absolute; color: #f0eff4; z-index: 998; line-height: 8px; font-size: 18px; right: 59.5px;bottom:-3px}


.yhq_btn {
  float: right;
  height: 90px;
  background-color: #d1121a;
  width: 65px;
  text-align: center;
}

.yhq_btn_gq {
  float: right;
  height: 90px;
  background-color: #fff;
  width: 65px;
  text-align: center;
  border-left: 1px dashed #ddd;
}

.yhq_btn_gq image {
  width: 45px;
  height: 45px;
  margin-top: 22.5px;
}

.yhq_btn_txt {
  font-size: 14px;
  color: #fff;
  position: absolute;
  top: 50%;
  margin-top: -27px;
  right: 26px;
}

.yhq_btn_txt2 {
  font-size: 14px;
  color: #fff;
  position: absolute;
  margin-top: -13px;
  right: 26px;
  width: 14px;
}

.yhq_tpye {
  float: left;
  width: 70px;
  height: 70px;
  margin: 10px 10px;
  border-radius: 8px;
}

.yhq_tpye image {
  width: 70px;
  height: 70px;
}

.yhq_value_fa {
  float: left;
  width: 53%;
  margin-top: 3px;
}

.yhq_value {
  float: left;
  width: 100%;
  margin-top: 6px;
}

.yhq_value1 {
  font-size: 12px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
  line-height: 18px;
  color: #333;
}

.yhq_tpye image {
  width: 70px;
  height: 70px;
}

.yhq_value2 {
  font-size: 16px;
  line-height: 18px;
  color: #ffb71b;
}

.yhq_value3 {
  font-size: 14px;
  line-height: 18px;
  color: #ffb71b;
  vertical-align: bottom;
  margin-left: 10px;
}

.yhq_red_txt {
  color: #d1121a !important;
}

.yhq_gray_txt {
  color: #333 !important;
}

.yhq_bg_red {
  background-color: #d1121a !important;
}

.yhq_bg_gray {
  background-color: #d0d0d0 !important;
}

.yhq_kf {
  position: fixed;
  z-index: 999;
  right: 10px;
  bottom: 10%;
}

.tab_bg {
  position: fixed;
  z-index: 9999;
  background-color: #fff;
  font-size: 14px;
  line-height: 40px;
  text-align: center;
  width: 100%;
  height: 40px;
}

.tab_tip {
  position: fixed;
  z-index: 9999;
  background-color: #ffca28;
  color: #333;
  font-size: 12px;
  line-height: 25px;
  text-align: center;
  width: 100%;
  height: 25px;
}

.tab_list {
  float: left;
  width: 20%;
}

.tab_list2 {
  float: left;
  width: 25%;
}

.tab_list_select {
  color: #d1121a;
}

.yhq_none_fa {
  position: absolute;
  top: 36%;
  margin-top: -92px;
  width: 100%;
  height: auto;
  margin: 0 auto;
  text-align: center;
}

.loading {
  font-size: 14px;
  text-align: center;
  color: #999;
  padding: 5px 0px 15px 0px;
}

.yhq_bg_red {
  background-color: #d1121a !important;
}












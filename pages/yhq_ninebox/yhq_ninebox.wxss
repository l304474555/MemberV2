page{overflow:hidden;}

.cj_back{position: absolute; left: 10px;top: 10px;z-index: 10}

.cj_rule{position: absolute; right: 10px;top: 10px;z-index: 10}

.cj_prize{position: absolute; right: 10px;bottom: 10px; z-index: 10}

.cj_fa{ width: 90%; height: auto; margin: 0 auto;}

.cj_jf{clear: both; font-size: 30rpx; text-align: center;margin-top: 8px; }

.cj_list{ float: left; width: calc(100% / 3 - 10px); height: auto; margin: 5px 5px 0 5px;}

.cj_list image{ width:100%; height: auto;}

.cj_list_bg{width:90%;position: absolute;top: 110px;right: 0;left: 0;z-index:4; margin:0 auto;}

.cj_list_fa{float:left; width:90%;position: absolute;top:110px;z-index:5}








@media (max-width:320px){

  .cj_list_bg{width:90%;position: absolute;top: 80px;right: 0;left: 0;z-index:4; margin:0 auto;}
  
  .cj_list_fa{float:left; width:90%;position: absolute;top:80px;z-index:5}
    
}


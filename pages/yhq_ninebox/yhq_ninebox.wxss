/**头号玩家**/
.task_fa{ background-color:#fff;
border-radius:8px;
float:left;
width:70%;
margin:0 auto;
padding:15px 5%;
margin-left:10%;
position: fixed;
z-index: 999991;
top: 21%;
}

.t_title{ float: left; font-size: 32rpx;}

.t_close{ float: right; width: 40rpx;}

.task_list{ float: left; width: 100%;  border-radius: 10px; padding: 20px 0;margin-top: 20px; }

.task_list_l{ color: #fff; font-size: 28rpx; display: inline-block; width: 56%; margin-left: 30rpx; margin-right: 40rpx;  vertical-align: middle;}

.task_list_btn{ display: inline-block; vertical-align: middle; background-color: #fff; border-radius: 100px; padding: 8rpx 30rpx; font-size: 26rpx; line-height: normal;}

button[class="task_list_btn"]::after {
  border: 0;
} 

.bgcolor1{ color: #ff0000;background-color: #ff0000;}

.bgcolor2{ color: #ff6666;background-color: #ff6666;}

.bgcolor3{ color: #ff9966;background-color: #ff9966;}

.color1{ color: #ff0000;}

.color2{ color: #ff6666;}

.color3{ color: #ff9966;}

.color_act{ color: #fff; background-color: transparent; }

 
 a {
  background: #fff;
  margin: 20rpx;
  border: 1px solid #000000;
  width: 210rpx;
  height: 210rpx;
  box-sizing: border-box;
  display: block;
  float: left;
  text-align: center;
  line-height: 100px;
  position: relative;
  border-radius: 5rpx;
}
 
a:hover {
  cursor: pointer;
  color: orange;
  font-size: 18px;
}
 
.active {
  background: red;
  color: #fff;
}
 
#pp {
  line-height: 32px;
  color: #9a9a9a;
  text-align: center;
}
 
page view {
 
  margin-top: 0rpx;
  margin-left: 0rpx;
  margin-bottom: 0rpx;
  margin-right: 0rpx;
  box-sizing: border-box;
}


.indexli{
 opacity: 0.4;
}
 
 
a {
  position: relative;
}
 
a image {
  width: 100%;
  height: 100%;
  border-radius: 5rpx;
}
 
a view {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  font-size: 40rpx;
  color: #fff;
  font-weight: 700;

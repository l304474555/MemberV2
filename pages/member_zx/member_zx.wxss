page {  
  display: block;  
  min-height: 100%;  
  background-color: #fff;  
  font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
  color:#333;
}

.h_tw_banner{ display: inline-block;width: 100%; }

.h_zx_title{position: absolute; bottom:5px; width:calc(100% - 20px); padding: 5px 0; background-color: #000;background: rgba(0, 0, 0, .6);}

.h_zx_title_1{ color: #fff; font-size: 16px; margin-left: 10px;}
.h_zx_title_2{ color: #fff; font-size: 12px;  margin-left: 10px;clear: both;display:block;white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}


.h_zx_list_fa{position: relative; display: inline-block; width:calc(100% - 20px); height: auto;  padding:12px 10px; background-color: #fff;border-bottom:1px solid #f0eff4}

.h_zx_list_banner{width:140px; vertical-align: middle;}


.h_zx_list_value{ display: inline-block; width: 50%; vertical-align: top;}
.h_zx_list_txt1{ display: inline-block; width: 100%;  font-size: 15px; margin-left: 10px;}
.h_zx_list_txt2{ display: -webkit-box;width: 100%; height: 32px; font-size: 12px; color: #707070; margin-left: 10px; margin-top: 5px;overflow: hidden;text-overflow: ellipsis;-webkit-line-clamp: 2; -webkit-box-orient: vertical;word-break: break-all;}

.h_zx_list_img{ position: absolute; bottom:10px; right: 10px; width:20px; }
.yhq_kf {
position:fixed;
z-index:999;
right:10px;
bottom:1%;

}
.h_zx_list_img_fa{position:absolute;
bottom:0px;
right:10px;
width:30px;
height:30px;
text-align:center;
}


/*屏幕响应判断 iphone5 兼容 */
@media (max-width:330px){
.h_zx_list_banner{width:120px; vertical-align: middle;}
.h_zx_list_value{ display: inline-block; width: 45%; vertical-align: top;}
}








} */

.memberCard>.position>.positionBody>.positionIcon {
  width: 35rpx;
  vertical-align: middle;
  max-height: 50rpx;
}

.memberCard>.position>.positionBody>text {
  vertical-align: middle;
  display: inline-block;
  font-size: 26rpx;
  color: #9a8367;
}

.memberCard>.position>.positionBody>.postionMore {
  width: 10rpx;
  vertical-align: middle;
  margin-left: 5rpx;
  max-height: 50rpx;
}

.memberCard>.memberInfo {
  position: absolute;
  top: 20rpx;
  left: 45rpx;
  z-index: 1;
}

.memberCard>.memberInfo>image {
  height: 15px;
  width: 15px;
  display: inline-block;
  vertical-align: middle;
}

.memberCard>.memberInfo>.memberText {
  display: inline-block;
  margin-left: 10px;
  vertical-align: middle;
  font-size: 30rpx;
}

.memberCard>.memberInfo>.memberText>text {
  color: #fff4e6;
  font-family: 'Alibaba';
}


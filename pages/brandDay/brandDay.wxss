.backGroundImg {
  position: fixed;
  height: 100%;
  top: 0;
  width: calc(100vw);
  z-index: -1;
  overflow: scroll;
  display: block;
}

.movable-area {
  height: 100vh;
  width: calc(100vw);
  top: 0;
  overflow: scroll;
  position: absolute;
  /* pointer-events: none; */
}


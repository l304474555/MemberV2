.hw_dialog {
  position: absolute;
  z-index: 999;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
}

.hw_dialog .content {
  position: relative;
  z-index: 88;
  width: 100%;
  height: 1023rpx;
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/bg_dialog.png);
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
  box-sizing: border-box;
}

.hw_dialog .content .title {
  position: absolute;
  left: 220rpx;
  top: 138rpx;
  width: 196rpx;
  height: 53rpx;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
}

.hw_dialog .content .win-title {
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/text_congratulate.png);
}

.hw_dialog .content .choose-title {
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/text_choose.png);
}

.hw_dialog .content .runout-title {
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/text_retry.png);
}

.hw_dialog .content .fail-title {
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/text_sorry.png);
}

.hw_dialog .winPrize {
  padding-top: 254rpx;
}

.hw_dialog .winPrize .poster {
  width: 246rpx;
  height: 333rpx;
  margin: 0 auto;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
}

.hw_dialog .winPrize .tips {
  text-align: center;
  position: relative;
  color: #eab880;
  margin: 0 auto 0;
  font-size: 24rpx;
  width: 320rpx;
  background-color: #fef5e1;
  z-index: 2;
}

.hw_dialog .winPrize .tips::before {
  content: '';
  position: absolute;
  left: 0;
  margin: 11rpx auto 0;
  border-top: 2rpx solid #eab880;
  width: 42rpx;
  z-index: -1;
}

.hw_dialog .winPrize .tips::after {
  content: '';
  position: absolute;
  right: 0;
  margin: 11rpx auto 0;
  border-top: 2rpx solid #eab880;
  width: 42rpx;
  z-index: -1;
}

.hw_dialog .winPrize .control .btn {
  width: 250rpx;
  height: 80rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
}

.hw_dialog .winPrize .control .replay {
  margin: 10rpx auto 0;
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/btn_replay.png);
}

.hw_dialog .winPrize .control .share {
  margin: 20rpx auto 0;
  background-color: transparent;
  border: none;
  outline: none;
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/btn_share.png);
}

.hw_dialog .winPrize .control .opentype {
  opacity: 0;
  margin-top: -80rpx;
}

.hw_dialog .winPrize .control .ticket {
  margin: 10rpx auto 0;
  background-image: url(https://mimage.myj.com.cn/MicroMallFileServer/Files/Excel/UploadExcel/201807/e226ced57980218d.png);
}

.hw_dialog .choose {
  padding-top: 300rpx;
}

.hw_dialog .choose .prizeTable {
  height: 516rpx;
  margin: 20rpx auto 0;
  transform: translateX(20rpx);
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
}

.hw_dialog .choose .prizeTable .card {
  width: 180rpx;
  height: 242rpx;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/bg_gift1.png);
}

.hw_dialog .choose .tips {
  text-align: center;
  position: relative;
  color: #fb8e08;
  margin: 0 auto 0;
  font-size: 40rpx;
  width: 508rpx;
  background-color: #fef5e1;
  z-index: 2;
  transform: translateX(20rpx);
}

.hw_dialog .fail {
  padding-top: 350rpx;
}

.hw_dialog .fail .poster {
  margin: 0 auto;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
  transform: translateX(20rpx);
}

.hw_dialog .fail .runout-poster {
  width: 233rpx;
  height: 242rpx;
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/bg_runout.png);
}

.hw_dialog .fail .fail-poster {
  width: 360rpx;
  height: 355rpx;
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/bg_none.png);
}

.hw_dialog .fail .tips {
  font-size: 28rpx;
  color: #e49325;
  text-align: center;
  transform: translateX(20rpx);
}

.hw_dialog .fail .runout-tips {
  margin: 50rpx auto 0;
}

.hw_dialog .fail .fail-tips {
  margin: -30rpx auto 0;
}

.hw_dialog .fail .btn {
  width: 250rpx;
  height: 80rpx;
  margin: 48rpx auto 0;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
  transform: translateX(20rpx);
}

.hw_dialog .fail .runout-btn {
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/btn_yes.png);
}

.hw_dialog .fail .fail-btn {
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/btn_replay.png);
}

.hw_dialog .close {
  position: relative;
  z-index: 55;
  width: 88rpx;
  height: 88rpx;
  position: relative;
  margin: 36rpx auto 0;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url(https://24haowan-cdn.shanyougame.com/dingzhi/myj21/images/icon_close.png);
}

.hw_dialog .close::after {
  content: '';
  position: absolute;
  width: 0;
  height: 100rpx;
  border-left: 4rpx solid #fff4e1;
  left: 43rpx;
  bottom: 88rpx;
  z-index: 44;
}

page{background-color: #ebebeb; font-size: 30rpx;}

.fun_type_list{position: relative; background-color: #fff; border-bottom: 1px solid #ddd; padding: 16px 10px; }

.fun_type_list font{color: #ffbc2b;}

.fun_type_select{position: absolute; right: 10px; width: 40rpx; height: 40rpx; top: 50%; margin-top: -20rpx;}

.fun_agreement{ margin-top: 5px;}

.fun_agreement image{ vertical-align: middle; width: 30rpx; margin-left: 5px;}

.fun_agreement font{ vertical-align: middle; font-size: 26rpx;}

.fun_button_fa{position: absolute; bottom: 20px; left: 0; right: 0; width: 95%; margin: 0 auto;}

.fun_button{background: linear-gradient(to right, #ffbc2b , #ffe04e); color: #533a00;padding:10px !important; border-radius: 5px; font-size: 32rpx !important; line-height: normal !important; }

.fun_modal_fa{
position: absolute;
z-index: 999999;
top: 20%;
left:0;
right:0;
margin: 0 auto;
background-color: #fff;
border-radius: 8px;
width: 90%;
height: auto;
text-align: center;
}

.fun_title{ background: linear-gradient(to right, #ffbc2b , #ffe04e); color: #533a00;padding:6px 30px; border-radius: 50px; font-size: 32rpx; display: inline-block;}

.fun_button_l{ display: inline-block; background:#2ba245; color: #fff;padding:10px !important; border-radius: 5px; font-size: 32rpx !important; line-height: normal !important; width: 43%;border: 1px solid #2ba245; }

button[class="fun_button_l"]::after {
  border: 0;
} 


.fun_button_r{display: inline-block;background:#fff; color: #000;padding:10px !important; border-radius: 5px; font-size: 32rpx !important; line-height: normal !important; width: 43%; border: 1px solid #929292; }

button[class="fun_button_r"]::after {
  border: 0;
} 

/*这个一定要 要不然扫不出来*/
.barcode > canvas {
  
    width: 640rpx;
    height: 150rpx;
    margin: 0 auto; 
}

.jf_close{
  position:absolute;
width:23px;
height:23px;
right:-5px;
top:-35px;
z-index:99999;

}

/*屏幕响应判断 iphone5 兼容 */
@media (max-width:330px){
  /*这个一定要 要不然扫不出来*/
.barcode > canvas {
    width: 640rpx;
    height: 200rpx;  
}
}







.yhq_dq_fa_l{display:inline-block; width:110rpx;vertical-align: top;}
.yhq_dq_fa_m{display:inline-block; width:66%; margin-left: 10px; vertical-align: top;}

.yhq_dq_shop_name{ display: inline-block; vertical-align: middle; font-size: 32rpx; font-weight: bold;}
.yhq_dq_shop_address{display: inline-block; width: 100%; vertical-align: middle; font-size: 26rpx; color: #747474; margin-top: 3px;}
.yhq_dq_icon{display: inline-block; font-size: 22rpx; background-color: #ffdf6f; border: 1px solid #bc981d; color: #744d01; text-align: center; padding: 1px 5px; border-radius: 5px; vertical-align: middle; margin-left: 8px;}

.yhq_dq_list_jl{position: absolute; right: 10px; top: 10px;}